{
  "strategy": "FreqAI_Hybrid_Conservative_RL",
  "max_open_trades": 3,
  "stake_currency": "USDT",
  "stake_amount": "unlimited",
  "tradable_balance_ratio": 0.85,
  "fiat_display_currency": "AUD",
  "timeframe": "5m",
  "dry_run": true,
  "dry_run_wallet": 10000,
  "cancel_open_orders_on_exit": true,
  "trading_mode": "spot",
  "margin_mode": "",
  
  "unfilledtimeout": {
    "entry": 10,
    "exit": 15,
    "exit_timeout_count": 2,
    "unit": "minutes"
  },
  
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": true,
      "bids_to_ask_delta": 1.2
    }
  },
  
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1
  },
  
  "exchange": {
    "name": "binance",
    "key": "",
    "secret": "",
    "ccxt_config": {},
    "ccxt_async_config": {},
    "pair_whitelist": [
      "BTC/USDT",
      "ETH/USDT",
      "ADA/USDT",
      "SOL/USDT",
      "LINK/USDT"
    ],
    "pair_blacklist": [
      "BNB/.*",
      ".*/BTC",
      ".*/ETH"
    ]
  },
  
  "freqai": {
    "enabled": true,
    "identifier": "hybrid_rl_conservative",
    "startup_candles": 6000,
    "train_period_days": 30,
    "backtest_period_days": 10,
    "purge_old_models": 2,
    "save_backtest_models": true,
    "write_metrics_to_disk": true,
    "data_kitchen_thread_count": 4,
    
    "model_training_parameters": {
      "learning_rate": 0.0003,
      "batch_size": 512,
      "n_steps": 2048,
      "gamma": 0.99,
      "n_epochs": 10,
      "device": "auto"
    },
    
    "rl_config": {
      "model_type": "PPO",
      "policy_type": "MlpPolicy",
      "train_cycles": 200,
      "max_trade_duration_candles": 80,
      "model_save_type": "stable_baselines3",
      "add_state_info": false,
      "net_arch": [256, 256],
      
      "model_reward_parameters": {
        "rr": 2.5,
        "profit_aim": 0.02,
        "win_reward_factor": 12,
        "punish_factor": 3
      },
      
      "randomize_starting_position": false
    },
    
    "feature_parameters": {
      "include_timeframes": ["5m", "1h", "4h"],
      "include_corr_pairlist": ["ETH/USDT", "BTC/USDT"],
      "label_period_candles": 20,
      "include_shifted_candles": 2,
      "principal_component_analysis": false,
      "use_SVM_to_remove_outliers": true,
      "indicator_periods_candles": [10, 20, 50],
      "DI_threshold": 0.8,
      "weight_factor": 0,
      "plot_feature_importances": 0,
      "shuffle_after_split": false,
      "buffer_train_data_candles": 0
    },
    
    "data_split_parameters": {
      "test_size": 0.25,
      "shuffle": false,
      "random_state": 1337
    }
  },
  
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  
  "telegram": {
    "enabled": true,
    "token": "",
    "chat_id": ""
  },
  
  "api_server": {
    "enabled": true,
    "listen_ip_address": "127.0.0.1",
    "listen_port": 8080,
    "verbosity": "error",
    "enable_openapi": false,
    "jwt_secret_key": "your_secret_key_here",
    "ws_token": "your_ws_token_here",
    "CORS_origins": [],
    "username": "freqtrader",
    "password": "your_secure_password"
  },
  
  "bot_name": "HybridRL_Conservative",
  "initial_state": "running",
  "force_entry_enable": false,
  
  "internals": {
    "process_throttle_secs": 5
  }
}
